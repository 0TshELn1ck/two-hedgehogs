{% extends(':Admin:base.html.twig') %}
{% block content %}
    <h3>Add new category for dish</h3>
    {{ form_start(form) }}
    {{ form_widget(form) }}
    <input type="submit" value="Create"
           class="btn btn-default"/>
    {{ form_end(form) }}
    <br>
    {% if msg != "" %}
        <p><b>{{ msg }}</b></p>
    {% endif %}
{% endblock content %}
