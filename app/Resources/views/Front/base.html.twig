{% extends('base.html.twig') %}

{% block stylesheets %}
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link href="{{ asset('css/reset.css') }}" rel="stylesheet"/>
    <link href="{{ asset('css/bootstrap.css') }}" rel="stylesheet"/>
    <link href="{{ asset('css/font-awesome.min.css') }}" rel="stylesheet"/>
    <link href="{{ asset('css/resto.css') }}" rel="stylesheet"/>
    {% block new_stylesheets %}{% endblock %}
    <link href="{{ asset('css/style.css') }}" rel="stylesheet"/>
{% endblock stylesheets %}

{% block javascripts %}
    <script src="{{ asset('js/jquery.js') }}"></script>
    <script src="{{ asset('js/bootstrap.js') }}"></script>
    <script src="{{ asset('js/easing.js') }}"></script>
    {% block new_js %}{% endblock %}
    <script src="{{ asset('js/main.js') }}"></script>
    {% block initialize_plagins %}{% endblock %}
{% endblock javascripts %}

{% block body %}
    {% block header %}{% include ':Front:header.html.twig' %}{% endblock header %}

    {% block content %}{% endblock content %}

    {% block footer %}{% include ':Front:footer.html.twig' %}{% endblock footer %}
{% endblock body %}


